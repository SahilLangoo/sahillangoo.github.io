{
  "name": "sahil-langoo-personal-website",
  "type": "module",
  "homepage": "https://sahillangoo.com",
  "version": "1.3.0",
  "private": true,
  "description": "Sahil's personal website built with Astro and Tailwind CSS",
  "author": "Sahil Langoo",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/sahillangoo/sahillangoo.github.io.git"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.50.0"
  },
  "scripts": {
    "dev": "astro dev",
    "preview": "astro preview",
    "build": "astro build && jampack ./dist",
    "lint": "eslint --ext .astro,.js,.ts .",
    "format:check": "prettier --check .",
    "format": "prettier --write . --plugin=prettier-plugin-astro",
    "sync": "astro sync",
    "check": "astro check",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{astro,html,js,jsx,svelte,ts,tsx,vue}": [
      "prettier --write ."
    ]
  },
  "dependencies": {
    "@astrojs/check": "^0.4.1",
    "@astrojs/mdx": "^2.3.0",
    "@astrojs/rss": "^4.0.5",
    "@astrojs/sitemap": "^3.1.2",
    "@astrojs/tailwind": "^5.1.0",
    "@divriots/jampack": "^0.23.4",
    "@tailwindcss/typography": "^0.5.12",
    "astro": "^4.6.1",
    "astro-breadcrumbs": "^1.10.0",
    "astro-favicons": "^2.0.2",
    "astro-icon": "^1.1.0",
    "astro-min": "^1.2.6",
    "astro-webmanifest": "^1.0.0",
    "daisyui": "^4.10.1",
    "mdast-util-to-string": "^4.0.0",
    "reading-time": "^1.5.0",
    "rehype-external-links": "^3.0.0",
    "sharp": "^0.33.3",
    "tailwindcss": "^3.4.3",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^7.6.0",
    "@typescript-eslint/parser": "^7.6.0",
    "astro-eslint-parser": "^0.16.3",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-astro": "^0.31.4",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.2",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "prettier-plugin-astro": "^0.12.3",
    "prettier-plugin-astro-organize-imports": "^0.3.3",
    "prettier-plugin-css-order": "^2.1.2",
    "prettier-plugin-tailwindcss": "^0.5.13",
    "typescript": "^5.4.5"
  },
  "packageManager": "pnpm@8.15.7+sha256.50783dd0fa303852de2dd1557cd4b9f07cb5b018154a6e76d0f40635d6cee019"
